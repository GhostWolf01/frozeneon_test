{
  "name": "frozeneon_test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.16.2",
    "vue": "^3.2.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.16.0",
    "sass": "^1.38.0",
    "sass-loader": "^10.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/max-attributes-per-line": [
        "error",
        {
          "multiline": {
            "allowFirstLine": true
          }
        }
      ],
      "vue/html-indent": [
        "error",
        2,
        {
          "alignAttributesVertically": false
        }
      ],
      "vue/attributes-order": [
        "error",
        {
          "order": [
            [
              "OTHER_ATTR",
              "GLOBAL",
              "DEFINITION",
              "LIST_RENDERING",
              "UNIQUE",
              "CONDITIONALS",
              "RENDER_MODIFIERS",
              "SLOT",
              "TWO_WAY_BINDING",
              "OTHER_DIRECTIVES",
              "EVENTS",
              "CONTENT"
            ]
          ]
        }
      ],
      "vue/valid-v-slot": [
        "error",
        {
          "allowModifiers": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
